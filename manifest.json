{
  "name": "GitHub File2PDF",
  "description": "Easily turn displaying markdown / source code file on GitHub to printable PDF!",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Print this GitHub page",
      "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
